#: upstream/_versions/3.0/guides/rabbitmq-reference.adoc:720
#, no-wrap
msgid "Attribute (_alias_)"
msgstr "属性 (_alias_)"

#. type: Table
#: upstream/_versions/3.0/guides/rabbitmq-reference.adoc:468
#: upstream/_versions/3.0/guides/rabbitmq-reference.adoc:720
#, no-wrap
msgid "Description"
msgstr "説明"

