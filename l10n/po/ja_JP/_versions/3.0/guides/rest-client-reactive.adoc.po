#: upstream/_versions/3.0/guides/rest-client-reactive.adoc:414
#, no-wrap
msgid ""
"@Provider\n"
"public class CustomHttpClientOptions implements ContextResolver<HttpClientOptions> {\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/3.0/guides/rest-client-reactive.adoc:422
#: upstream/_versions/3.0/guides/rest-client-reactive.adoc:465
#, no-wrap
msgid ""
"    @Override\n"
"    public HttpClientOptions getContext(Class<?> aClass) {\n"
"        HttpClientOptions options = new HttpClientOptions();\n"
