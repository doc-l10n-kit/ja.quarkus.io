msgstr "**ERROR** と同じ"

#. type: Plain Text
#: upstream/_versions/2.7/guides/logging.adoc:152
msgid "WARNING"
msgstr "WARNING"

#. type: Plain Text
#: upstream/_versions/2.7/guides/logging.adoc:152
msgid "Same as **WARN**."
msgstr "**WARN** と同じ"

#. type: Plain Text
#: upstream/_versions/2.7/guides/logging.adoc:153
msgid "CONFIG"
msgstr "CONFIG"

#. type: Plain Text
#: upstream/_versions/2.7/guides/logging.adoc:153
msgid "Service configuration information."
msgstr "サービス構成情報"

